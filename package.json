{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . --platform=win32 --icon=myicon.ico --win32metadata.OriginalFilename=HelloWorld.exe --win32metadata.FileDescription=HelloWorld --win32metadata.ProductName=HelloWorld --overwrite --ignore=./*.md --ignore=.gitignore --ignore=.idea --download.mirror=https://npm.taobao.org/mirrors/electron/"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.6.2",
    "electron-packager": "^8.5.1"
  },
  "dependencies": {
    "electron": "^1.6.2"
  }
}
